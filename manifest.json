{
    "manifest_version": 3,
    "name": "MSDocs Easy Switch Lang",
    "version": "1.0.1",
    "description": "Easy switch lang button on msdocs for Microsoft Edge.",
    "default_locale": "en",
    "icons": {
        "128": "icons/128.png",
        "48": "icons/48.png",
        "16": "icons/16.png"
    },    
    "action": {
    },
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "host_permissions": [
        "https://docs.microsoft.com/*"
    ],
    "background": {
        "service_worker": "scripts/background.js"
    }
}